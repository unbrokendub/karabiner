{
    "title": "Insert to CMD+SHIFT+R",
    "rules": [
      {
        "description": "Change insert to CMD + SHIFT + R (Render in Ableton Live)",
        "manipulators": [
            {
                "type": "basic",
                "from": {
                  "key_code": "insert",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },

          "to": [
            {
              "key_code": "r",
              "modifiers": [
                "left_command",
                "shift"
              ]
            },
            {
              "key_code": "vk_none"
            }
          ]
        }
      ]
    }
  ]
}